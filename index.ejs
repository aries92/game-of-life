                            <button type="submit" id="setRandomCells" class="btn btn-primary">
                                Add
                            </button>
                        </div>
                    </div>
                </div>

                <div class="card mb-3">
                    <div class="card-header">
                        Edit Cells
                    </div>
                    <div class="card-body">
                        <button type="submit" id="addCells" class="btn btn-primary">
                            Add cells
                        </button>
                        <button type="submit" id="removeCells" class="btn btn-secondary">
                            Remove cells
                        </button>
                    </div>
                </div>

                <div class="card mb-3">
                    <div class="card-header">
                        Change Speed
                    </div>
                    <div class="card-body">
                        <input type="number" value="1" id="speedChange">ms
                    </div>
                </div>

                <div class="text-center mt-5">
                    <div class="btn-group" role="group" aria-label="Basic example">
                        <button type="button" id="start" class="btn btn-success">Start</button>
                        <button type="button" id="pause" class="btn btn-warning">Pause</button>
                        <button type="button" id="stop" class="btn btn-danger">Stop</button>
                    </div>
                </div>

            </div>
